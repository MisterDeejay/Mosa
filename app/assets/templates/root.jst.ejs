<section class="launch-app section">
  <div class="jumbotron vertical-center">
    <div class='container signin text-center'>
      <div class="row">
        <div class="col-xs-12">
          <img class="logo" src="<%= asset_path('mosa_logo_high_res_rgb-8.png') %>" alt="mosa_logo" />
        </div>
      </div>

      <form class="find-location" action="index.html" method="post">
        <div class="row">
          <div class="col-xs-12">
            <button type="button" class="btn btn-primary current-location">
              Use Current Location
            </button>
          </div>
        </div>

        <div class="row">
          <div class="col-xs-12">
            <input type="text" class="zipcode" name="zipcode"
              placeholder="Enter Your Zipcode">
          </div>
        </div>

        <div class="row">
          <div class="col-xs-12">
            <button class="submit" name="submit">Submit!</button>
          </div>
        </div>
      </form>
    </div>
  </div>
</section>
